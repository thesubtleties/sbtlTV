{
  "name": "sbtltv",
  "version": "0.1.0",
  "private": true,
  "description": "Desktop IPTV player with mpv",
  "scripts": {
    "dev": "pnpm --filter @sbtltv/electron dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "dist": "pnpm build && pnpm --filter @sbtltv/electron dist",
    "dist:win": "pnpm build && pnpm --filter @sbtltv/electron dist:win",
    "dist:mac": "pnpm build && pnpm --filter @sbtltv/electron dist:mac",
    "dist:linux": "pnpm build && pnpm --filter @sbtltv/electron dist:linux"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
