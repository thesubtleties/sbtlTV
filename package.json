{
  "name": "sbtltv",
  "version": "0.1.0",
  "private": true,
  "description": "Desktop IPTV player with mpv",
  "homepage": "https://github.com/thesubtleties/sbtlTV",
  "author": {
    "name": "thesubtleties",
    "email": "thesubtleties@users.noreply.github.com"
  },
  "scripts": {
    "dev": "node scripts/dev.cjs",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "dist": "pnpm build && pnpm --filter @sbtltv/electron dist",
    "dist:win": "pnpm build && pnpm --filter @sbtltv/electron dist:win",
    "dist:mac": "pnpm build && pnpm --filter @sbtltv/electron dist:mac",
    "dist:linux": "pnpm build && pnpm --filter @sbtltv/electron dist:linux"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
